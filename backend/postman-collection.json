                  "raw": "{\n  \"title\": \"ID Recovery Application Form\",\n  \"description\": \"Form for citizens to apply for ID recovery services\",\n  \"sections\": [\n    {\n      \"title\": \"Personal Information\",\n      \"description\": \"Please provide your personal details\",\n      \"pageNumber\": 1,\n      \"orderIndex\": 1,\n      \"fields\": [\n        {\n          \"label\": \"Full Name\",\n          \"fieldName\": \"full_name\",\n          \"fieldType\": \"text\",\n          \"isRequired\": true,\n          \"placeholder\": \"Enter your full name\",\n          \"helpText\": \"As it appears on your original documents\",\n          \"orderIndex\": 1\n        },\n        {\n          \"label\": \"Email Address\",\n          \"fieldName\": \"email\",\n          \"fieldType\": \"email\",\n          \"isRequired\": true,\n          \"placeholder\": \"your@email.com\",\n          \"orderIndex\": 2\n        },\n        {\n          \"label\": \"Phone Number\",\n          \"fieldName\": \"phone\",\n          \"fieldType\": \"phone_number\",\n          \"isRequired\": true,\n          \"placeholder\": \"+1234567890\",\n          \"orderIndex\": 3\n        }\n      ]\n    },\n    {\n      \"title\": \"Document Upload\",\n      \"description\": \"Please upload required documents\",\n      \"pageNumber\": 2,\n      \"orderIndex\": 2,\n      \"fields\": [\n        {\n          \"label\": \"Birth Certificate\",\n          \"fieldName\": \"birth_certificate\",\n          \"fieldType\": \"document_upload\",\n          \"isRequired\": true,\n          \"helpText\": \"Upload a clear copy of your birth certificate\",\n          \"orderIndex\": 1\n        },\n        {\n          \"label\": \"Proof of Address\",\n          \"fieldName\": \"address_proof\",\n          \"fieldType\": \"document_upload\",\n          \"isRequired\": false,\n          \"helpText\": \"Utility bill or bank statement\",\n          \"orderIndex\": 2\n        }\n      ]\n    }\n  ]\n}"
